
@{
    ViewData["Title"] = "Profile";
    var appSession = ViewBag.Session as EasyAssetManagerCore.Model.CommonModel.AppSession;
}
<link rel="stylesheet" type="text/css" href="~/ApplicationTheme/assets/styles/profile/customer.min.css">
<div class="ks-header">
    <section class="ks-title">
        <h3>Agent Profile</h3>
    </section>
</div>
