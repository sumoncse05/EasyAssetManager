        rowArray.push([25, 34, 2]);
        rowArray.push([34, 36, 2]);
        rowArray.push([36, 38, 2]);
        rowArray.push([1, 3, 3]);
        rowArray.push([3, 5, 3]);
        rowArray.push([11, 13, 3]);
        rowArray.push([13, 15, 3]);
        rowArray.push([15, 17, 3]);
        rowArray.push([17, 19, 3]);
        rowArray.push([19, 21, 3]);
        rowArray.push([25, 27, 3]);
        rowArray.push([27, 29, 3]);
        rowArray.push([29, 31, 3]);
        rowArray.push([31, 33, 3]);

        colArray.push([3, 4, 5]);
        colArray.push([3, 4, 6]);
        colArray.push([3, 4, 7]);
        colArray.push([3, 4, 8]);
        colArray.push([3, 4, 9]);
        colArray.push([3, 4, 10]);
        colArray.push([3, 4, 21]);
        colArray.push([3, 4, 22]);
        colArray.push([3, 4, 23]);
        colArray.push([3, 4, 24]);
        colArray.push([3, 4, 33]);
        colArray.push([3, 4, 34]);
        colArray.push([3, 4, 35]);
        colArray.push([3, 4, 36]);
        colArray.push([3, 4, 37]);
        title = ApplicationCommon.reportTitleAndHeaderCustomize();
        ApplicationCommon.makePagination('tb-assetatGlanceReport', 3, rowArray, colArray, title);

        var rowCount = $('#tb-assetatGlanceReport >tbody >tr').length;
        if (rowCount > 0) {
            $("#sidebarCollapse").addClass('fa-chevron-down').removeClass('fa-chevron-up');
            $("#divsidebarCollapse").addClass('border-bottom');
            $('#sidebarContent').css('display', 'none');
        }

        $("#trBranchPop").hide();
    });

</script>
