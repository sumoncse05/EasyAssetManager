@model IEnumerable<EasyAssetManagerCore.Models.EntityModel.Process>
@{
    Layout = null;
}
@if (Model != null && Model.Any())
{
    <table class="table table-striped table-bordered table-hover dataTables-example table-responsive" id="tb-accrunningprocess">
        <thead>
            <tr>
                <th>Run ID</th>
                <th>Status</th>
                <th>Program</th>
                <th>Completed</th>                
            </tr>
        </thead>
        <tbody>
            @foreach (var v in Model)
            {
                <tr>
                    <td>@v.RUN_ID</td>
                    <td>@v.STATUS_TEXT</td>
                    <td>@v.PROGRAM_NAME</td>
                    <td>@v.COMPLETED </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No record found.</p>
}


<script type="text/javascript">
    $(document).ready(function () {        
        ApplicationCommon.makePagination('tb-accrunningprocess');

        var rowCount = $('#tb-accrunningprocess >tbody >tr').length;
        if (rowCount > 0) {
            $("#sidebarCollapse").addClass('fa-chevron-down').removeClass('fa-chevron-up');
            $("#divsidebarCollapse").addClass('border-bottom');
            $('#sidebarContent').css('display', 'none');
        }
    });
    
</script>
