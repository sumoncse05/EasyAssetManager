@model IEnumerable<EasyAssetManagerCore.Models.EntityModel.AST_LOAN_CL_TMP>
@{
    Layout = null;
}
@if (Model != null && Model.Any())
{
    <table class="table table-striped table-bordered table-hover dataTables-example table-responsive" id="tb-cllist" width="100%">
        <thead>
            <tr>
                @*<th>AREA_CODE</th>
                <th>AREA_NAME</th>
                <th>BRANCH_CODE</th>
                <th>BRANCH_NAME</th>*@
                <th>Loan Number</th>
                @*<th>RM_CODE</th>
                <th>RM_NAME</th>
                <th>BST_CODE</th>
                <th>BST_NAME</th>*@
                <th>CL Status</th>
                <th>EFF Date</th>
                <th>Ins By</th>
                <th>Ins Date</th>
                <th>Edit</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var v in Model)
            {
            <tr>
                @*<td>@v.AREA_CODE</td>
                <td>@v.AREA_NAME</td>
                <td>@v.BRANCH_CODE</td>
                <td>@v.BRANCH_NAME</td>*@
                <td>@v.LOAN_AC_NUMBER</td>
                @*<td>@v.RM_CODE</td>
                <td>@v.RM_NAME</td>
                <td>@v.BST_CODE</td>
                <td>@v.BST_NAME</td>*@
                <td>@v.CL_STATUS</td>
                <td>@v.EFF_DATE</td>
                <td>@v.INS_BY</td>
                <td>@v.INS_DATE</td>
                <td>

                    <a href="@Url.Action("Index","LoanCl",new {loan_number= v.LOAN_AC_NUMBER})" class="btn btn-success gridButton"> <i class="fa fa-edit"></i></a>
                </td>
            </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No record found.</p>
}

<script type="text/javascript">
    $(document).ready(function () {
        ApplicationCommon.makePagination('tb-cllist');
        var rowCount = $('#tb-cllist >tbody >tr').length;
        if (rowCount > 0) {
            $("#sidebarCollapse").addClass('fa-chevron-down').removeClass('fa-chevron-up');
            $("#divsidebarCollapse").addClass('border-bottom');
            $('#sidebarContent').css('display', 'none');
        }

        $("#trBranchPop").hide();
    });

</script>